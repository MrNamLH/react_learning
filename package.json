		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-prettier": "^2.6.2",
		"eslint-plugin-react": "^7.9.1",
		"flow-bin": "^0.77.0",
		"husky": "^0.14.3",
		"lint-staged": "^7.2.0",
		"npm-run-all": "^4.1.3",
		"prettier": "^1.13.7",
		"stylelint": "^9.3.0",
		"stylelint-config-standard": "^18.2.0"
	}
