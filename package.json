	"license": "MIT",
	"dependencies": {
		"bootstrap": "3",
		"prop-types": "^15.6.2",
		"react": "^16.4.1",
		"react-bootstrap": "^0.32.1",
		"react-dom": "^16.4.1",
		"react-redux": "^5.0.7",
		"react-scripts": "1.1.4",
		"redux": "^4.0.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"shortid": "^2.2.12"
	},
	"scripts": {
		"precommit": "lint-staged",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"lint:js:fix": "eslint . --fix --ignore-path .gitignore --ext=js --ext=jsx",
		"lint:css:fix": "stylelint \"**/*.css\" --fix",
		"test:lint:js": "eslint . --ignore-path .gitignore --ext=js --ext=jsx",
		"test:lint:css": "stylelint \"**/*.css\"",
		"test:unit": "react-scripts test --env=jsdom",
		"test": "run-s test:**",
		"eject": "react-scripts eject"
	},
	"lint-staged": {
		"*.{js,jsx,json,css}": [
			"prettier --config .prettierrc --write",
			"git add"
		]
	},
	"flow": "flow",
	"devDependencies": {
		"eslint": "^5.1.0",
		"eslint-config-airbnb": "^17.0.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-config-react": "^1.1.7",
